<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="200.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.useCaseDiagramControllers.UseCaseDiagramToolsController">
   <children>
      <Label alignment="CENTER" prefHeight="37.0" prefWidth="100.0" text="Tools" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#createActorListener" prefHeight="25.0" prefWidth="185.0" text="Actor" />
      <Button mnemonicParsing="false" onAction="#createUseCaseLIstener" prefHeight="25.0" prefWidth="139.0" text="Use Case" />
      <Button mnemonicParsing="false" onAction="#createIncludeListener" prefHeight="25.0" prefWidth="167.0" text="Include" />
      <Button mnemonicParsing="false" onAction="#createExcludeListener" prefHeight="25.0" prefWidth="190.0" text="Exclude" />
      <Button mnemonicParsing="false" onAction="#createGeneralizationListener" prefHeight="25.0" prefWidth="111.0" text="Generalization" />
      <Button mnemonicParsing="false" onAction="#createSystemBoundaryListener" prefHeight="25.0" prefWidth="100.0" text="System Boundary">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
   </children>
</VBox>
